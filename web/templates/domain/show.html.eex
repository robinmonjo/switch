<h2>Show domain</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @domain.name %>
  </li>

  <li>
    <strong>Redirect:</strong>
    <%= @domain.redirect %>
  </li>

  <li>
    <strong>Name checked:</strong>
    <%= @domain.name_checked %>
  </li>

  <li>
    <strong>Redirect checked:</strong>
    <%= @domain.redirect_checked %>
  </li>

</ul>

<%= link "Edit", to: domain_path(@conn, :edit, @domain) %>
<%= link "Back", to: domain_path(@conn, :index) %>
